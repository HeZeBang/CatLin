{
  "version": 2,
  "rewrites": [
    {
      "source": "/:uri",
      "destination": "/api"
    }
  ],
  "headers": [
    {
      "source": "/(.*).br",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "br"
        },
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        }
      ]
    }
  ]
}