{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/provider/:uri",
      "destination": "https://d.zambar.top/api/:uri"
    }
  ],
  "headers": [
    {
      "source": "/(.*).br",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "br"
        },
        {
          "key": "Cache-Control",
          "value": "max-age=2592000"
        },
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        }
      ]
    }
  ]
}